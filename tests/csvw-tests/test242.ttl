@prefix csvw: <http://www.w3.org/ns/csvw#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

 [
    a csvw:TableGroup;
    csvw:table [
      a csvw:Table;
      rdfs:label "datatype @id an absolute URL that does not resolve";
      rdfs:comment "If included, @id is a link property that identifies the datatype described by this datatype description.";
      csvw:row [
        a csvw:Row;
        csvw:describes [ <test242.csv#string> "foo"^^<http://example.org/datatpe>];
        csvw:rownum 1;
        csvw:url <test242.csv#row=2>
      ];
      csvw:url <test242.csv>
    ]
 ] .
