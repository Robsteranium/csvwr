 [
    a csvw:TableGroup;
    csvw:table [
      a csvw:Table;
      rdfs:label "invalid #0.0#,# 12.34,567";
      rdfs:comment "A number format pattern as defined in [UAX35]. Implementations MUST recognise number format patterns containing the symbols `0`, `#`, the specified decimalChar (or `.` if unspecified), the specified groupChar (or `,` if unspecified), `E`, `+`, `%` and `&permil;`.";
      csvw:row [
        a csvw:Row;
        csvw:describes [ <http://www.w3.org/2013/csvw/tests/test304.csv#%230.0%23> "12.34,567"];
        csvw:rownum "1"^^xsd:integer;
        csvw:url <http://www.w3.org/2013/csvw/tests/test304.csv#row=2>
      ];
      csvw:url <http://www.w3.org/2013/csvw/tests/test304.csv>
    ]
 ] .
