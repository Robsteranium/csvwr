@prefix csvw: <http://www.w3.org/ns/csvw#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

 [
    a csvw:TableGroup;
    rdfs:label "This metadata is used";
    rdfs:comment "User-specified metadata does not need to reference the starting CSV";
    csvw:table [
      a csvw:Table;
      csvw:row [
        a csvw:Row;
        csvw:describes [
          <test121-ref.csv#countryCode> "YE";
          <test121-ref.csv#latitude> "15"^^xsd:double;
          <test121-ref.csv#longitude> "48"^^xsd:double;
          <test121-ref.csv#name> "Yemen"
        ];
        csvw:rownum 1;
        csvw:url <test121-ref.csv#row=2>
      ],  [
        a csvw:Row;
        csvw:describes [
          <test121-ref.csv#countryCode> "ZM";
          <test121-ref.csv#latitude> "-15"^^xsd:double;
          <test121-ref.csv#longitude> "30"^^xsd:double;
          <test121-ref.csv#name> "Zambia"
        ];
        csvw:rownum 2;
        csvw:url <test121-ref.csv#row=3>
      ],  [
        a csvw:Row;
        csvw:describes [
          <test121-ref.csv#countryCode> "ZW";
          <test121-ref.csv#latitude> "-20"^^xsd:double;
          <test121-ref.csv#longitude> "30"^^xsd:double;
          <test121-ref.csv#name> "Zimbabwe"
        ];
        csvw:rownum 3;
        csvw:url <test121-ref.csv#row=4>
      ];
      csvw:url <test121-ref.csv>
    ]
 ] .
