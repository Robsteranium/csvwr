@prefix csvw: <http://www.w3.org/ns/csvw#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<test306.csv#about> <test306.csv#value> ("1" "2" "3"),  ("4" "5" "6"),  ("a" "b" "c"),  ("d" "e" "f") .

 [
    a csvw:TableGroup;
    csvw:table [
      a csvw:Table;
      rdfs:label "multiple values with same subject and property (ordered)";
      rdfs:comment "Values in separate columns using the same propertyUrl are kept in proper relative order.";
      csvw:row [
        a csvw:Row;
        csvw:describes <test306.csv#about>;
        csvw:rownum "1"^^xsd:integer;
        csvw:url <test306.csv#row=2>
      ],  [
        a csvw:Row;
        csvw:describes <test306.csv#about>;
        csvw:rownum "2"^^xsd:integer;
        csvw:url <test306.csv#row=3>
      ];
      csvw:url <test306.csv>
    ]
 ] .
