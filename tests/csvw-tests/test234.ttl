@prefix csvw: <http://www.w3.org/ns/csvw#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

 [
    a csvw:TableGroup;
    csvw:table [
      a csvw:Table;
      rdfs:label "multiple column primaryKey violation";
      rdfs:comment "Validators MUST raise errors if there is more than one row with the same primary key.";
      csvw:row [
        a csvw:Row;
        csvw:describes [
          <test234.csv#PK1> "foo";
          <test234.csv#PK2> "bar"
        ];
        csvw:rownum 1;
        csvw:url <test234.csv#row=2>
      ],  [
        a csvw:Row;
        csvw:describes [
          <test234.csv#PK1> "foo";
          <test234.csv#PK2> "bar"
        ];
        csvw:rownum 2;
        csvw:url <test234.csv#row=3>
      ];
      csvw:url <test234.csv>
    ]
 ] .
