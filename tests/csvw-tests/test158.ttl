@prefix csvw: <http://www.w3.org/ns/csvw#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

 [
    a csvw:TableGroup;
    csvw:table [
      a csvw:Table;
      rdfs:label "number format (valid combinations)";
      rdfs:comment "Numeric dataype with object format.";
      csvw:row [
        a csvw:Row;
        csvw:describes [
          <test158.csv#decimal> "0.1"^^xsd:decimal;
          <test158.csv#decimal1> "10000.1"^^xsd:decimal;
          <test158.csv#double> "0.0e0"^^xsd:double;
          <test158.csv#integer> "0"^^xsd:integer
        ];
        csvw:rownum 1;
        csvw:url <test158.csv#row=2>
      ],  [
        a csvw:Row;
        csvw:describes [
          <test158.csv#decimal> "10.1"^^xsd:decimal;
          <test158.csv#decimal1> "1.1"^^xsd:decimal;
          <test158.csv#double> "10.10e1"^^xsd:double;
          <test158.csv#integer> "10"^^xsd:integer
        ];
        csvw:rownum 2;
        csvw:url <test158.csv#row=3>
      ];
      csvw:url <test158.csv>
    ]
 ] .
